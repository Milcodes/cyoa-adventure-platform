{
  "name": "cyoa-adventure-platform",
  "version": "1.0.0",
  "description": "Interactive Choose Your Own Adventure game platform with visual editor",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "devDependencies": {
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.0",
    "@types/uuid": "^10.0.0",
    "prettier": "^3.2.4",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "cyoa",
    "choose-your-own-adventure",
    "interactive-fiction",
    "game-engine",
    "story-editor",
    "web-game",
    "react",
    "nestjs",
    "typescript"
  ],
  "author": "Milcodes",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Milcodes/cyoa-adventure-platform.git"
  },
  "bugs": {
    "url": "https://github.com/Milcodes/cyoa-adventure-platform/issues"
  },
  "homepage": "https://github.com/Milcodes/cyoa-adventure-platform#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@nestjs/platform-express": "^11.1.8",
    "multer": "^2.0.2",
    "uuid": "^13.0.0"
  }
}
